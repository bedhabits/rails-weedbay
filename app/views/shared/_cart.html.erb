<table>
  <tbody id="cart-table">
    <%= render(cart.product_orders) %>
  </tbody>
    <tr class="total_line">
      <td colspan="2">
        <strong>Total</strong>
      </td>
      <td class="total_cell">
        <strong><%= cart.sub_total %></strong>
      </td>
    </tr>
</table>
<div class="d-flex justify-content-between mt-2 mb-2">
  <%= button_to "Checkout", new_order_path, class: "btn btn-primary", method: :get %>
  <%= button_to "Empty cart", cart_path(cart), class: "btn btn-link", method: :delete, data: {confirm: "Are you sure?" } %>
</div>
